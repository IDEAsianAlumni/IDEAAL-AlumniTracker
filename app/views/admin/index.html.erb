<h1>ADMIN PAGE</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>UID</th>
      <th>Major</th>
      <th>Resume</th>
      <th>Phone Number</th>
      <th>Prof Track</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.uid %></td>
        <td><%= user.Major %></td>
        <td><%= user.resume_link %></td>
        <td><%= user.phone_number %></td>
        <td><%= user.Professional_Track %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Back to home", root_path %>