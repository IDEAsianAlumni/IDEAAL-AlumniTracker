        <div class="field">
          <%= f.label :email %>
          <%= f.email_field :email, autofocus: true %>
        </div>
        <div class="field">
          <%= f.label :first_name %>
          <%= f.text_field :first_name, autofocus: true %>
        </div>

        <div class="field">
          <%= f.label :last_name %>
          <%= f.text_field :last_name, autofocus: true %>
        </div>

        <div class="field">
          <%= f.label :password %>
          <% if @minimum_password_length %>
          <em>- minimum of <%= @minimum_password_length %> characters</em>
          <% end %>
          <%= f.password_field :password, autocomplete: "new-password" %>
        </div>

