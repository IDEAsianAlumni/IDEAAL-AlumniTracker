        <div class="field">
          <%= f.label :Resume%>
          <em>- optional</em>
          <em>- paste as a link</em>
          <em>(consider LinkedIn)</em>
          <%= f.text_field :resume_link, autofocus: true %>
        </div>

        <div class="field">
          <%= f.label :phone %>
          <em>- optional</em>
          <em>- format: "111-111-1111"</em>
          <%= f.text_field :phone_number, autofocus: true %>
        </div>

        <br>
        <div class="actions">
          <%= f.submit "Sign Up" , :class=> "btn btn-outline-secondary"%>
        </div>
      <% end %>
    
    <%= render "devise/shared/links" %><br>

  </div>
